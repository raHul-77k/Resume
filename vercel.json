{
    "version": 2,
    "builds": [
        {
            "src": "Resume/wsgi.py",
            "use": "@vercel/pyhton",
            "config":{"maxLambdaSize": "15mb", "runtime": "python3.8"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config":{"disDir": "staticfiles_build"}
        }
    ],
    "routes": [
        {
        "src": "/static/(.*)",
        "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Resume/wsgi.py"
        }
    ]
}